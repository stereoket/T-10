<?xml version="1.0" encoding="UTF-8"?>
<ti:app xmlns:ti="http://ti.appcelerator.org">
    <id>com.stereoartist.t10</id>
    <name>T-10</name>
    <version>1.0</version>
    <publisher>Team Flipside</publisher>
    <url>http://www.TeeMinusTen.com</url>
    <description>International Space Apps Challenge Winning Team from London</description>
    <copyright>Team Flipside</copyright>
    <icon>appicon.png</icon>
    <persistent-wifi>false</persistent-wifi>
    <prerendered-icon>false</prerendered-icon>
    <statusbar-style>default</statusbar-style>
    <statusbar-hidden>false</statusbar-hidden>
    <fullscreen>false</fullscreen>
    <navbar-hidden>false</navbar-hidden>
    <analytics>true</analytics>
    <guid>b11f1bae-85ce-473e-b1f6-1040291123fe</guid>
    <property name="ti.ui.defaultunit" type="string">dp</property>
  <iphone>
        <orientations device="iphone">
            <orientation>Ti.UI.PORTRAIT</orientation>
        </orientations>
        <orientations device="ipad">
            <orientation>Ti.UI.PORTRAIT</orientation>
            <orientation>Ti.UI.UPSIDE_PORTRAIT</orientation>
        </orientations>
    </iphone>
    <ios>
        <plist>
            <dict>
                <key>UIBackgroundModes</key>
                <array>
                    <string>audio</string>
                    <string>location</string>
                </array>
                <key>UIAppFonts</key>
                <array>
                    <string>/fonts/OstrichSans-Edit.ttc</string>
                    <!-- <string>/fonts/ostrich-black.ttf</string> -->
                </array>
            </dict>
        </plist>
        <min-ios-ver>5.0</min-ios-ver>
    </ios>
    <android xmlns:android="http://schemas.android.com/apk/res/android"/>
    <mobileweb>
        <precache/>
        <splash>
            <enabled>true</enabled>
            <inline-css-images>true</inline-css-images>
        </splash>
        <theme>default</theme>
    </mobileweb>
    <modules>
        <module platform="commonjs">ti.cloud</module>
        <module platform="iphone"  version="0.4">bencoding.localnotify</module>
    </modules>
    <deployment-targets>
        <target device="blackberry">false</target>
        <target device="android">false</target>
        <target device="ipad">true</target>
        <target device="iphone">false</target>
        <target device="mobileweb">true</target>
        <target device="tizen">false</target>
    </deployment-targets>
    <sdk-version>3.1.0.GA</sdk-version>
    <plugins>
        <plugin version="1.0">ti.alloy</plugin>
    </plugins>
</ti:app>
